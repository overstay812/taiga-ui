        "addon-editor": {
            "implicitDependencies": []
        },
        "addon-mobile": {
            "implicitDependencies": []
        },
        "addon-table": {
            "implicitDependencies": []
        },
        "addon-tablebars": {
            "implicitDependencies": []
        },
        "cdk": {
            "implicitDependencies": []
        },
        "core": {
            "implicitDependencies": []
        },
        "i18n": {
            "implicitDependencies": []
        },
        "icons": {
            "implicitDependencies": []
        },
        "kit": {
            "implicitDependencies": []
        },
        "taiga-schematics": {
            "implicitDependencies": []
        },
        "testing": {
            "implicitDependencies": []
        },
        "demo": {
            "implicitDependencies": []
        }
    },
    "affected": {
        "defaultBase": "main"
    },
    "workspaceLayout": {
        "libsDir": "projects",
        "appsDir": "projects"
    }
