        "angular.json": "*",
        "package.json": {
            "dependencies": "*",
            "devDependencies": "*"
        },
        "tslint.json": "*",
        "nx.json": "*",
        "tsconfig.*.json": "*",
        "tsconfig.json": "*"
    },
    "npmScope": "taiga-ui",
